{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "React Chrome Extension"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}